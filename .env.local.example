# Scandit SDK License Key (optional - native BarcodeDetector is used by default)
# Get your license key from https://www.scandit.com/
NEXT_PUBLIC_SCANDIT_LICENSE_KEY=your-scandit-license-key-here

# App URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000
TELEGRAM_BOT_WEBHOOK_URL=https://your-bot-webhook-url.railway.app

# Airtable Configuration
# Get these from https://airtable.com/account
AIRTABLE_TOKEN=patXXXXXXXXXXXXXXXX.your-airtable-personal-access-token
AIRTABLE_BASE_ID=appXXXXXXXXXXXXXX
AIRTABLE_INVENTORY_TABLE=tblXXXXXXXXXXXXXX
AIRTABLE_TRANSACTIONS_TABLE=tblXXXXXXXXXXXXXX
AIRTABLE_USER_STATE_TABLE=tblXXXXXXXXXXXXXX
AIRTABLE_BOX_INVENTORY_TABLE=tblXXXXXXXXXXXXXX
AIRTABLE_UNIDENTIFIED_BOXES_TABLE=tblXXXXXXXXXXXXXX

# Cloudinary Configuration
# Get these from https://cloudinary.com/console
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Upstash Redis Configuration (Vercel KV)
# Get these from your Vercel project settings or Upstash console
KV_REST_API_URL=https://your-redis-instance.upstash.io
KV_REST_API_TOKEN=your-upstash-token

# OpenRouter API Configuration
# Required for LLM cross-validation during invoice matching
# Get your API key from https://openrouter.ai/
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here
